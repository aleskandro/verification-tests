{
        "kind": "Pod",
        "apiVersion":"v1",
        "metadata": {
                "name": "hello-pod",
                "labels": {
                        "name": "hello-pod"
                }
        },
        "spec": {
                "containers": [{
                        "name": "hello-pod",
                        "image": "quay.io/aleskandrox/hello-pod:centos-8080",
                        "ports": [
                        {
                                "hostPort": 4789,
                                "containerPort": 8080,
                                "protocol": "UDP"
                        }
                        ]
                }]
        }
}
